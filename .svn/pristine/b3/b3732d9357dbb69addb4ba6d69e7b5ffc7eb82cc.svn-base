using System.IO;

namespace TeamTickets.Entity
{
    public class IDCard
    {
        public string Code { get; set; }
        public string Name { get; set; } = "";
        /// <summary>
        /// 性别
        /// </summary>
        public string Gender { get; set; }
        /// <summary>
        /// 民族
        /// </summary>
        public string Flok { get; set; }
        public string BirthDay { get; set; }
        public string Address { get; set; }
        /// <summary>
        /// 有效期开始
        /// </summary>
        public string ExpireStart { get; set; }
        /// <summary>
        /// 有效期结束
        /// </summary>
        public string ExpireEnd { get; set; }
        /// <summary>
        /// 签证机关
        /// </summary>
        public string Agency { get; set; }
        /// <summary>
        /// 照片目录
        /// </summary>
        public string PhotoDirectory { get; set; } = Directory.GetCurrentDirectory();
    }
}
